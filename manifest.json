{
    "name": "MasterMoviesHD",
    "version": "1.0.0",
    "description": "Watch Online Movies",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": ["https://*.youtube.com/watch?*"],
            "js": ["content.js"],
            "css": ["content.css"],
            "run_at": "document_end"
        }
    ],
    "icons": {
        "16": "Icon.png",
        "48": "Icon.png",
        "128": "Icon.png"
    },
    "browser_action": {
        "default_title": "MasterFilmesHD"
    },
    "web_accessible_resources": ["script.js"]
}